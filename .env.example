# Copy this file to .env and update with your values

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/fly_elysia_react

# Authentication
BETTER_AUTH_SECRET=your-secret-key-min-32-chars-change-in-production
BETTER_AUTH_URL=http://localhost:3001

# Redis Queue Security
# IMPORTANT: Change this password in production!
REDIS_PASSWORD=dev-redis-password-change-in-production
REDIS_URL=redis://:dev-redis-password-change-in-production@localhost:6379

# Worker Configuration
RUN_WORKERS=true

# Node Environment
NODE_ENV=development